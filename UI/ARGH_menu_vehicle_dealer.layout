FrameWidgetClass {
 Name "ARGH_VehicleDealerMenuRoot"
 Slot FrameWidgetSlot "{ARGH-VD-ROOT}" {
  Anchor 0 0 1 1
 }
 {
  ImageWidgetClass "{ARGH-VD-DIM}" {
   Name "DimBackground"
   Slot FrameWidgetSlot "{ARGH-VD-DIM-SLOT}" {
    Anchor 0 0 1 1
   }
   Color 0 0 0 0.85
  }
  FrameWidgetClass "{ARGH-VD-DIALOG}" {
   Name "DialogFrame"
   Slot FrameWidgetSlot "{ARGH-VD-DIALOG-SLOT}" {
    Anchor 0.1 0.1 0.9 0.9
   }
   {
    ImageWidgetClass "{ARGH-VD-BG}" {
     Name "DialogBackground"
     Slot FrameWidgetSlot "{ARGH-VD-BG-SLOT}" {
      Anchor 0 0 1 1
      OffsetRight 0
      OffsetBottom 0
     }
     Color 0.02 0.024 0.028 0.98
    }
    VerticalLayoutWidgetClass "{ARGH-VD-MAIN}" {
     Name "MainLayout"
     Slot FrameWidgetSlot "{ARGH-VD-MAIN-SLOT}" {
      Anchor 0 0 1 1
      OffsetRight 0
      OffsetBottom 0
     }
     {
      SizeLayoutWidgetClass "{ARGH-VD-HEADER-SIZE}" {
       Name "HeaderSize"
       Slot LayoutSlot "{ARGH-VD-HEADER-SIZE-SLOT}" {
       }
       AllowHeightOverride 1
       HeightOverride 48
       {
        OverlayWidgetClass "{ARGH-VD-HEADER}" {
         Name "Header"
         Slot AlignableSlot "{ARGH-VD-HEADER-SLOT}" {
          HorizontalAlign 3
          VerticalAlign 3
         }
         {
          ImageWidgetClass "{ARGH-VD-HEADER-BG}" {
           Name "HeaderBg"
           Slot OverlayWidgetSlot "{ARGH-VD-HEADER-BG-SLOT}" {
            HorizontalAlign 3
            VerticalAlign 3
           }
           Color 0.03 0.035 0.04 1
          }
          TextWidgetClass "{ARGH-VD-TITLE}" {
           Name "TitleText"
           Slot OverlayWidgetSlot "{ARGH-VD-TITLE-SLOT}" {
            HorizontalAlign 3
            VerticalAlign 3
            Padding 16 0 0 0
           }
           Color 0.85 0.85 0.85 1
           Text "VEHICLE DEALER"
           "Font Size" 20
           "Horizontal Alignment" Left
           "Vertical Alignment" Center
          }
          ButtonWidgetClass "{ARGH-VD-CLOSE}" {
           Name "CloseButton"
           Slot OverlayWidgetSlot "{ARGH-VD-CLOSE-SLOT}" {
            HorizontalAlign 2
            VerticalAlign 1
            Padding 0 8 16 0
           }
           style blank
           {
            TextWidgetClass "{ARGH-VD-CLOSE-TXT}" {
             Name "CloseText"
             Slot ButtonWidgetSlot "{ARGH-VD-CLOSE-TXT-SLOT}" {
              HorizontalAlign 3
              VerticalAlign 3
             }
             Color 0.6 0.6 0.6 1
             Text "X"
             "Font Size" 16
            }
           }
          }
         }
        }
       }
      }
      HorizontalLayoutWidgetClass "{ARGH-VD-BODY}" {
       Name "BodyLayout"
       Slot LayoutSlot "{ARGH-VD-BODY-SLOT}" {
        SizeMode Fill
        FillWeight 1
       }
       {
        SizeLayoutWidgetClass "{ARGH-VD-CATS-SIZE}" {
         Name "CategoriesSize"
         Slot LayoutSlot "{ARGH-VD-CATS-SIZE-SLOT}" {
         }
         AllowWidthOverride 1
         WidthOverride 180
         {
          OverlayWidgetClass "{ARGH-VD-CATS}" {
           Name "CategoriesPanel"
           Slot AlignableSlot "{ARGH-VD-CATS-SLOT}" {
            HorizontalAlign 3
            VerticalAlign 3
           }
           {
            ImageWidgetClass "{ARGH-VD-CATS-BG}" {
             Name "CategoriesBg"
             Slot OverlayWidgetSlot "{ARGH-VD-CATS-BG-SLOT}" {
              HorizontalAlign 3
              VerticalAlign 3
             }
             Color 0.025 0.03 0.035 1
            }
            VerticalLayoutWidgetClass "{ARGH-VD-CATS-LIST}" {
             Name "CategoryList"
             Slot OverlayWidgetSlot "{ARGH-VD-CATS-LIST-SLOT}" {
              HorizontalAlign 3
              VerticalAlign 0
              Padding 8 8 8 8
             }
             {
              ButtonWidgetClass "{ARGH-VD-CAT-ALL}" {
               Name "CategoryAll"
               Slot LayoutSlot "{ARGH-VD-CAT-ALL-SLOT}" {
                Padding 0 0 0 4
               }
               {
                TextWidgetClass "{ARGH-VD-CAT-ALL-TXT}" {
                 Name "CategoryAllText"
                 Slot ButtonWidgetSlot "{ARGH-VD-CAT-ALL-TXT-SLOT}" {
                  HorizontalAlign 0
                  VerticalAlign 3
                  Padding 8 6 8 6
                 }
                 Color 0.7 0.7 0.7 1
                 Text "All Vehicles"
                 "Font Size" 13
                }
               }
              }
             }
            }
           }
          }
         }
        }
        FrameWidgetClass "{ARGH-VD-GRID-AREA}" {
         Name "VehicleGridArea"
         Slot LayoutSlot "{ARGH-VD-GRID-AREA-SLOT}" {
          SizeMode Fill
          FillWeight 1
         }
         {
          ImageWidgetClass "{ARGH-VD-GRID-BG}" {
           Name "GridBg"
           Slot FrameWidgetSlot "{ARGH-VD-GRID-BG-SLOT}" {
            Anchor 0 0 1 1
           }
           Color 0.015 0.018 0.022 1
          }
          VerticalLayoutWidgetClass "{ARGH-VD-GRID}" {
           Name "VehicleGrid"
           Slot FrameWidgetSlot "{ARGH-VD-GRID-SLOT}" {
            Anchor 0 0 1 1
            OffsetLeft 12
            OffsetTop 12
            OffsetRight -12
            OffsetBottom -12
           }
          }
         }
        }
        SizeLayoutWidgetClass "{ARGH-VD-DETAILS-SIZE}" {
         Name "DetailsSize"
         Slot LayoutSlot "{ARGH-VD-DETAILS-SIZE-SLOT}" {
         }
         AllowWidthOverride 1
         WidthOverride 280
         {
          OverlayWidgetClass "{ARGH-VD-DETAILS}" {
           Name "DetailsPanel"
           Slot AlignableSlot "{ARGH-VD-DETAILS-SLOT}" {
            HorizontalAlign 3
            VerticalAlign 3
           }
           {
            ImageWidgetClass "{ARGH-VD-DETAILS-BG}" {
             Name "DetailsBg"
             Slot OverlayWidgetSlot "{ARGH-VD-DETAILS-BG-SLOT}" {
              HorizontalAlign 3
              VerticalAlign 3
             }
             Color 0.025 0.03 0.035 1
            }
            VerticalLayoutWidgetClass "{ARGH-VD-DETAILS-CONTENT}" {
             Name "DetailsContent"
             Slot OverlayWidgetSlot "{ARGH-VD-DETAILS-CONTENT-SLOT}" {
              HorizontalAlign 3
              VerticalAlign 0
              Padding 12 12 12 12
             }
             {
              TextWidgetClass "{ARGH-VD-VNAME}" {
               Name "VehicleName"
               Slot LayoutSlot "{ARGH-VD-VNAME-SLOT}" {
                Padding 0 0 0 8
               }
               Color 0.85 0.85 0.85 1
               Text "Select a vehicle"
               "Font Size" 16
              }
              TextWidgetClass "{ARGH-VD-VPRICE}" {
               Name "VehiclePrice"
               Slot LayoutSlot "{ARGH-VD-VPRICE-SLOT}" {
                Padding 0 0 0 16
               }
               Color 0.4 0.7 0.4 1
               Text "$0"
               "Font Size" 18
              }
              ButtonWidgetClass "{ARGH-VD-BUY-BTN}" {
               Name "BuyButton"
               Slot LayoutSlot "{ARGH-VD-BUY-BTN-SLOT}" {
               }
               {
                OverlayWidgetClass "{ARGH-VD-BUY-OL}" {
                 Name "BuyOverlay"
                 Slot ButtonWidgetSlot "{ARGH-VD-BUY-OL-SLOT}" {
                  HorizontalAlign 3
                  VerticalAlign 3
                 }
                 {
                  ImageWidgetClass "{ARGH-VD-BUY-BG}" {
                   Name "BuyBg"
                   Slot OverlayWidgetSlot "{ARGH-VD-BUY-BG-SLOT}" {
                    HorizontalAlign 3
                    VerticalAlign 3
                   }
                   Color 0.04 0.06 0.04 1
                  }
                  TextWidgetClass "{ARGH-VD-BUY-TXT}" {
                   Name "BuyText"
                   Slot OverlayWidgetSlot "{ARGH-VD-BUY-TXT-SLOT}" {
                    HorizontalAlign 3
                    VerticalAlign 3
                    Padding 16 10 16 10
                   }
                   Color 0.5 0.8 0.5 1
                   Text "PURCHASE"
                   "Font Size" 14
                  }
                 }
                }
               }
              }
              TextWidgetClass "{ARGH-VD-WALLET-LBL}" {
               Name "WalletLabel"
               Slot LayoutSlot "{ARGH-VD-WALLET-LBL-SLOT}" {
                Padding 0 16 0 2
               }
               Color 0.6 0.6 0.6 1
               Text "Wallet"
               "Font Size" 12
              }
              TextWidgetClass "{ARGH-VD-WALLET-VALUE}" {
               Name "WalletValue"
               Slot LayoutSlot "{ARGH-VD-WALLET-VALUE-SLOT}" {
                Padding 0 0 0 10
               }
               Color 0.75 0.75 0.75 1
               Text "$0"
               "Font Size" 14
              }
              TextWidgetClass "{ARGH-VD-BANK-LBL}" {
               Name "BankLabel"
               Slot LayoutSlot "{ARGH-VD-BANK-LBL-SLOT}" {
                Padding 0 6 0 2
               }
               Color 0.6 0.6 0.6 1
               Text "Bank"
               "Font Size" 12
              }
              TextWidgetClass "{ARGH-VD-BANK-VALUE}" {
               Name "BankValue"
               Slot LayoutSlot "{ARGH-VD-BANK-VALUE-SLOT}" {
                Padding 0 0 0 10
               }
               Color 0.75 0.75 0.75 1
               Text "$0"
               "Font Size" 14
              }
              TextWidgetClass "{ARGH-VD-STATUS}" {
               Name "StatusText"
               Slot LayoutSlot "{ARGH-VD-STATUS-SLOT}" {
                Padding 0 8 0 0
               }
               Color 0.7 0.55 0.3 1
               Text ""
               "Font Size" 12
              }
             }
            }
           }
          }
         }
        }
       }
      }
     }
    }
   }
  }
 }
}
